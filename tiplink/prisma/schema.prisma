generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model wallet {
  id     String  @id @default(dbgenerated("generate_uid((nextval('id_serial'::regclass))::integer, 6, 'tiplink_salt'::text)"))
  pubkey String?
  cipher String?
  salt   String?
}
